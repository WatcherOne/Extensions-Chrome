{
    "manifest_version": 3,
    "name": "test-plugin",
    "version": "1.0.0",
    "author": "watcher",
    "description": "a chrome plugin for test",
    "action": {
        "default_popup": "html/popup.html"
    },
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/jquery-3.6.0.min.js", "js/content.js"],
            "run_at": "document_start"
        }
    ],
    "default_locale": "zh_CN"
}